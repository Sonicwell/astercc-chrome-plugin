*{margin:0;padding:0;}
body{color:#333;overflow: hidden;margin: 0px;padding:5px;background: white;font-size:12px;width:306px}
.fullwid{
	width: 100%;
}


#myTabs{
	width:296px;
}

#myTabs .input-wrap{
	
}

#myTabs .input-wrap input[type="text"]{
	margin:5px 0;
}


.grid9-wrap{
	width: 296px;
}

.grid9-wrap .cell{
	float:left;
	width:90px;
	height:90px;
	text-align:center;
	vertical-align:middle;
	border:1px solid #3C5560;
	padding:0;
	margin-right:10px;
	margin-top:10px;
	background:-webkit-gradient(linear, 0% 0%, 0% 100%,from(#3C5560), to(#77939D));
}
.grid9-wrap .cell:hover{
	background-color:#ccc;
}

.grid9-wrap .cell.r{
	margin-right:0;
}

.grid9-wrap .cell img.icon{
	width:45px;
	display:block;
	margin: 0 auto;
}

.grid9-wrap .cell a{
	width:100%;
	height:100%;
	display:inline-block;
	padding-top:14px;
	color:#fff;
	
}

.modal-dialog{
	width:100%;
	margin:0;
}
.modal-header{
	padding:6px 10px;	
}
.modal-body{
	padding:5px;
}
.modal-title{
	font-size:14px;
}
.modal-footer{
	padding:6px 10px;	
}
