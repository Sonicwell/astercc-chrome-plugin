{
    "name": "__MSG_pluginName__",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "__MSG_pluginDescription__",
    "default_locale": "en",
    "background": {
        "page": "background.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"    
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "__MSG_pluginName__",
        "default_popup": "popup.html"
    },
    "permissions": [
        "http://*/*",
        "tabs","notifications","background"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/1.4.1/jquery.min.js; https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js; object-src 'self'"
}
